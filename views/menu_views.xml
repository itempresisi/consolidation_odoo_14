    <menuitem id="menu_consolidation_analysis"
              name="Analysis"
              parent="menu_consolidation_root"
              sequence="20"/>
    
    <!-- Financial Analysis menu -->
    <menuitem id="menu_financial_analysis"
              name="Financial Analysis"
              parent="menu_consolidation_analysis"
              action="action_consolidation_analysis_form"
              sequence="10"/>
    
    <!-- Consolidation Reports menu -->
    <menuitem id="menu_consolidation_reports"
              name="Consolidation Reports"
              parent="menu_consolidation_analysis"
              action="action_consolidation_report_form"
              sequence="20"/>
</odoo>