<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Main menu -->
    <menuitem id="menu_consolidation_root"
              name="Financial Consolidation"
              web_icon="financial_consolidation,static/description/icon.png"
              sequence="55"/>
    
    <!-- Configuration menu -->
    <menuitem id="menu_consolidation_config"
              name="Configuration"
              parent="menu_consolidation_root"
              sequence="100"/>
    
    <!-- Consolidation Group menu -->
    <menuitem id="menu_consolidation_group"
              name="Consolidation Groups"
              parent="menu_consolidation_config"
              action="action_consolidation_group_form"
              sequence="10"/>
    
    <!-- Consolidation Accounts menu -->
    <menuitem id="menu_consolidation_account"
              name="Consolidation Accounts"
              parent="menu_consolidation_config"
              action="action_consolidation_account_form"
              sequence="20"/>
    
    <!-- Account Mappings menu -->
    <menuitem id="menu_consolidation_account_mapping"
              name="Account Mappings"
              parent="menu_consolidation_config"
              action="action_consolidation_account_mapping_form"
              sequence="30"/>
    
    <!-- Intercompany Relations menu -->
    <menuitem id="menu_intercompany_relation"
              name="Intercompany Relations"
              parent="menu_consolidation_config"
              action="action_intercompany_relation_form"
              sequence="40"/>
    
    <!-- Automatic Eliminations menu -->
    <menuitem id="menu_automatic_elimination"
              name="Automatic Eliminations"
              parent="menu_consolidation_config"
              action="action_automatic_elimination_form"
